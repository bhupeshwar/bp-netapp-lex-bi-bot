{
	"name": "Jobdonetoday_Intent",
        "description": "Jobdonetoday_Intent Recursively query the dataset",
        "conclusionStatement": {
          "messages": [
            {
              "content": "via code hook",
              "contentType": "PlainText"
            }
          ]
        },
        "fulfillmentActivity": {
          "codeHook": {
            "messageVersion": "1.0",
            "uri": "{{lambda-arn}}"
          },
          "type": "CodeHook"
        },
        "sampleUtterances": [
          "Is {job_name} jobs completed on {job_date}",
					"Is {job_name} jobs has been completed {job_date}"
          ],
    "slots": [
			{
					"slotType": "job_name",
					"name": "job_name",
					"slotConstraint": "Optional",
					"valueElicitationPrompt": {
							"maxAttempts": 2,
							"messages": [
									{
											"content": "The name of templates",
											"contentType": "PlainText"
									}
							]
					},
					"priority": 1,
					"slotTypeVersion": "$LATEST",
					"sampleUtterances": [],
					"description": "job_name"
			},
        {
            "slotType": "AMAZON.DATE",
            "name": "job_date",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
                "maxAttempts": 2,
                "messages": [
                    {
                        "content": "The completion date  of the Job",
                        "contentType": "PlainText"
                    }
                ]
            },
            "priority": 2,
            "sampleUtterances": [],
            "description": "job_date"
        }
    ]
}
