{
	"name": "Jobdonetime_Intent",
        "description": "Jobdonetime_Intent Recursively query the dataset",
        "conclusionStatement": {
          "messages": [
            {
              "content": "via code hook",
              "contentType": "PlainText"
            }
          ]
        },
        "fulfillmentActivity": {
          "codeHook": {
            "messageVersion": "1.0",
            "uri": "{{lambda-arn}}"
          },
          "type": "CodeHook"
        },
        "sampleUtterances": [
          "When {dl_name} jobs completed on {job_date}",
					"When {dl_name} jobs completed"
          ],
    "slots": [
        {
            "slotType": "dl_name",
            "name": "dl_name",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
                "maxAttempts": 2,
                "messages": [
                    {
                        "content": "The type of event",
                        "contentType": "PlainText"
                    }
                ]
            },
            "priority": 1,
            "slotTypeVersion": "$LATEST",
            "sampleUtterances": [],
            "description": "dl_name"
        },
				{
					"slotType": "AMAZON.DATE",
					"name": "job_date",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
                "maxAttempts": 2,
                "messages": [
                    {
                        "content": "The completion date  of the Job",
                        "contentType": "PlainText"
                    }
                ]
            },
            "priority": 2,
            "sampleUtterances": [],
            "description": "job_date"
        }
    ]
}
