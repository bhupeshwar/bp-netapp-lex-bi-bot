{
  "name": "BIBotNetApp",
  "description": "NetApp-BIBot - a SQL query bot for the JOB_DETAILS database",
  "intents": [
        {
            "intentVersion": "$LATEST",
            "intentName": "Hello_Intent"
        },
        {
            "intentVersion": "$LATEST",
	           "intentName": "Count_Intent"
        },
        {
            "intentVersion": "$LATEST",
	           "intentName": "Clone_Intent"
        },
        {
            "intentVersion": "$LATEST",
            "intentName": "Reset_Intent"
        },
        {
            "intentVersion": "$LATEST",
            "intentName": "Goodbye_Intent"
        },
        {
            "intentVersion": "$LATEST",
	           "intentName": "Refreshsequence_Intent"
        },
        {
            "intentVersion": "$LATEST",
	           "intentName": "Jobdone_Intent"
        },
        {
            "intentVersion": "$LATEST",
             "intentName": "Jobdonetime_Intent"
        }
  ],
  "locale": "en-US",
  "abortStatement": {
    "messages": [
      {
        "content": "Sorry, I could not understand. Goodbye.",
        "contentType": "PlainText"
      }
    ]
  },
  "childDirected": false,
  "clarificationPrompt": {
    "maxAttempts": 5,
    "messages": [
      {
        "content": "Come again?",
        "contentType": "PlainText"
      },
      {
        "content": "Sorry, can you please repeat that?",
        "contentType": "PlainText"
      }
    ]
  },
  "idleSessionTTLInSeconds": 300,
  "voiceId": "Matthew"
}
